<script>




    // Inline js 
    $(".mode_switcher").click(function(){
        if($(this).hasClass("active")){
            $(this).removeClass("active");
            $(this).find(".switch_ball").css({
                'left':'5px',
                'right':'unset'
            });
            $(".day_switches").css({'display':'flex'});
            $(".night_switches").css({'display':'none'});
        }else{
            $(this).addClass("active");
            $(this).find(".switch_ball").css({
                'right':'5px',
                'left': 'unset'
            });
            $(".day_switches").css({'display':'none'});
            $(".night_switches").css({'display':'flex'});
        }
    });
</script>
